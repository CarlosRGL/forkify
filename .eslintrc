{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "indent": ["error", 2],
    "no-unused-vars": 2,
    "no-undef": 2
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  }
}
